   "metadata": {},
   "outputs": [],
   "source": [
    "hits_y = dataframe.hits\n",
    "\n",
    "step = 200\n",
    "new_hits = []\n",
    "for i in range(step, dataframe.shape[0]):\n",
    "    hits = hits_y.iloc[i-step: i+1]\n",
    "    perc_high = np.percentile(hits, 99)\n",
    "    perc_low = np.percentile(hits, 1)\n",
    "    hits = hits[(hits > perc_low)&(hits < perc_high)]\n",
    "    mean = hits.mean()\n",
